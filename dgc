#!/bin/bash
# DGC - Terminal Entry Point
# Usage: ./DGC TUI

COMMAND=$(echo "$1" | tr '[:lower:]' '[:upper:]')

if [ "$COMMAND" == "TUI" ]; then
    export DGC_GOD_MODE=1
    export PYTHONPATH="/Users/dhyana/DHARMIC_GODEL_CLAW"
    python3 /Users/dhyana/DHARMIC_GODEL_CLAW/src/core/dgc_tui.py
else
    echo "Usage: ./DGC TUI"
fi